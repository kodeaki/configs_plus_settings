{  
  "toggles": {  
    "communication": {  
      "discord": {  
        "enable": true,  
        "match": [{"class": "discord"}],  
        "command": ["discord"],  
        "move": true  
      },  
      "whatsapp": {  
        "enable": false,  
        "match": [{"class": "whatsapp"}],  
        "move": true  
      },  
      "signal": {  
        "enable": false,  
        "match": [{"class": "signal"}],  
        "command": ["signal-desktop"],  
        "move": true  
      }  
    },  
    "music": {  
      "Youtube Music": {  
        "enable": true,  
        "match": [{"class": "com.github.th_ch.youtube_music"}],  
        "command": ["pear-desktop"],  
        "move": true  
      }  
    },  
    "teams": {  
      "teams-for-linux": {  
        "enable": false,  
        "match": [{"class": "teams-for-linux"}],  
        "command": ["teams-for-linux"],  
        "move": true  
      }  
    }  
  }  
}
